{
    "name": "dpc-sdp/baywatch",
    "description": "Configuration for integration with Bay hosting platform",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "drush/drush": "^10.3",
        "drupal/purge": "3.0",
        "drupal/purge_queuer_url": "^1.0@RC",
        "drupal/section_purger": "dev-updates_to_compatible_with_drupal_9",
        "drupal/redis": "^1.0",
        "drupal/smtp": "^1.0@RC",
        "drupal/queue_mail": "^1",
        "drupal/jwt": "^1.0",
        "drupal/config_split": "^1.4"
    },
    "exclude": [
        "drupal/section_purger"
    ],
    "repositories": [
        {
            "type": "composer",
            "url": "https://packages.drupal.org/8",
            "exclude": [
                "drupal/section_purger"
            ]
        },
        {
            "type": "git",
            "url": "https://git.drupalcode.org/issue/section_purge-3227034.git"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true
}
