{
    "name": "dpc-sdp/baywatch",
    "description": "Configuration for integration with Bay hosting platform",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "drush/drush": "^10.3",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_queuer_url": "^1.0@RC",
        "drupal/redis": "^1.0@RC",
        "drupal/section_purger": "0.2.3",
        "drupal/smtp": "^1.0@RC",
        "drupal/queue_mail": "^1",
        "drupal/jwt": "1.0-beta1",
        "drupal/config_split": "^1.4"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
