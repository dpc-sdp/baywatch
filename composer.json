{
    "name": "dpc-sdp/baywatch",
    "description": "Configuration for integration with Bay hosting platform",
    "type": "drupal-module",
    "license": "GPL-2.0-or-later",
    "require": {
        "drush/drush": "^10.3",
        "drupal/purge": "^3.0@beta",
        "drupal/purge_queuer_url": "^1.0@RC",
        "drupal/redis": "^1.0@RC",
        "drupal/section_purger": "^2.3.0@RC",
        "drupal/smtp": "^1.0@RC",
        "drupal/queue_mail": "^1",
        "drupal/jwt": "1.0-beta1",
        "drupal/config_split": "^1.4"
    },
    "repositories": {
        "drupal": {
            "type": "composer",
            "url": "https://packages.drupal.org/8"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "patches": {
            "drupal/section_purger": {
                "Construct method does not implement EntityTypeManagerManageInterface - https://www.drupal.org/project/section_purge/issues/3194851#comment-13976987": "https://www.drupal.org/files/issues/2021-01-27/3194851-update-construct.patch"
            }
        }
    }
}
